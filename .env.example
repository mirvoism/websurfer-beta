# Mac Studio LLM Endpoint Configuration
OPENAI_API_BASE=https://matiass-mac-studio.tail174e9b.ts.net/v1
OPENAI_API_KEY=ollama

# Available Models (choose one as default)
# - deepseek-r1     : DeepSeek R1 reasoning model
# - qwen3:32b       : Qwen 3 32B parameter model  
# - qwen25          : Qwen 2.5 model
# - llama4:scout    : Llama 4 Scout model
# - llama4:maverick : Llama 4 Maverick model
DEFAULT_MODEL=deepseek-r1

# Browser MCP Configuration
# Ensure Chrome browser has MCP extension installed
BROWSER_MCP_ENABLED=true
BROWSER_MCP_TIMEOUT=30

# Agent Configuration
HUMAN_LIKE_DELAYS=true
MAX_RETRIES=3
LOG_LEVEL=INFO
